        <field name="postlogistics_default_packaging_id" ref="swisspost_pri_packaging"></field>
        <field name="postlogistics_label_layout" ref="delivery_postlogistics.postlogistics_layout_option_a6"></field>
        <field name="postlogistics_output_format" ref="delivery_postlogistics.postlogistics_output_format_option_zpl2"></field>
        <field name="postlogistics_resolution" ref="delivery_postlogistics.postlogistics_output_resolution_option_300ppp"></field>
