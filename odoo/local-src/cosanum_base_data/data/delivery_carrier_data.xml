<?xml version="1.0"?>
<odoo noupdate="1">

    <record id="product_product_delivery_fees" model="product.product">
        <field name="name">Additional Fees</field>
        <field name="type">service</field>
        <field name="categ_id" ref="delivery.product_category_deliveries"/>
        <field name="sale_ok" eval="False"/>
        <field name="purchase_ok" eval="False"/>
        <field name="list_price">24.0</field>
    </record>

    <record id="product_product_delivery_fees_thermocare" model="product.product">
