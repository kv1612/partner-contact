        <field name="name" >Cosanum Palette</field>
        <field name="free_over" eval="True"/>
        <field name="amount">30.0</field>
        <field name="sequence">1</field>
        <field name="delivery_type">cosanum</field>
        <field name="invoice_policy">pricelist</field>
        <field name="product_id" ref="product_product_delivery_fees"/>
        <field name="tech_name">coosafleet</field>
        <field name="cosanum_default_packaging_id" ref="cosanum_base_data.product_packaging_cosapal"/>
    </record>

    <record id="delivery_carrier_coosafleet_rol" model="delivery.carrier">
