<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="aged_partner_balance_wizard" model="ir.ui.view">
      <field name="inherit_id" ref="account_financial_report.aged_partner_balance_wizard" />
      <field name="model">aged.partner.balance.wizard</field>
      <field name="arch" type="xml">

        <xpath expr="//group[@name='partner_filter']" position="before">
          <group name="user_filter" col="1">
            <label for="user_id"/>
            <field name="user_id" nolabel="1" options="{'no_create': True}"/>
          </group>
        </xpath>
      </field>
    </record>
</odoo>
