<odoo>

    <record id="cosanum_delivery.delivery_carrier_brauch_pallet" model="delivery.carrier">
        <field name="send_delivery_notice_on">custom</field>
        <field name="send_delivery_notice_picking_type_ids"
          eval="[(6, 0, [ref('cosanum_stock_picking_type.stock_picking_type_pack_pal')])]"/>
    </record>

    <record id="cosanum_delivery.delivery_carrier_brauch_rollercage" model="delivery.carrier">
        <field name="send_delivery_notice_on">custom</field>
        <field name="send_delivery_notice_picking_type_ids"
          eval="[(6, 0, [ref('cosanum_stock_picking_type.stock_picking_type_pack_rol')])]"/>
    </record>

    <record id="cosanum_delivery.delivery_carrier_brauch_rollercage_op" model="delivery.carrier">
        <field name="send_delivery_notice_on">custom</field>
        <field name="send_delivery_notice_picking_type_ids"
          eval="[(6, 0, [
            ref('cosanum_stock_picking_type.stock_picking_type_pack_op'),
          ref('cosanum_stock_picking_type.stock_picking_type_pick_pack_mo_op'),
          ])]"/>
    </record>

    <record id="cosanum_delivery.delivery_carrier_pandemic_stock" model="delivery.carrier">
        <field name="send_delivery_notice_on">custom</field>
        <field name="send_delivery_notice_picking_type_ids" eval="[(6, 0, [ref('cosanum_stock_picking_type.stock_picking_type_pack_pal')])]"/>
    </record>

</odoo>
