<?xml version="1.0"?>
<odoo noupdate="1">

  <function model="stock.picking.type" name="write">
      <value model="stock.picking.type"
          eval="obj().env.ref('stock.warehouse0').manu_type_id.id"/>
      <value model="stock.picking.type" eval="{
            'sequence': 100,
            'show_operations': True,
            'show_entire_packs': False,
            'color': 9,
      }"/>
  </function>

  <function model="stock.picking.type" name="write">
      <value model="stock.picking.type"
             eval="obj().env.ref('cosanum_stock_warehouse_data.warehouse_wer').manu_type_id.id"/>
      <value model="stock.picking.type" eval="{
            'sequence': 100,
            'show_operations': True,
            'show_entire_packs': False,
            'color': 9,
      }"/>
  </function>

  <function model="stock.picking.type" name="write">
      <value model="stock.picking.type"
             eval="obj().env.ref('stock.warehouse0').pbm_type_id.id"/>
      <value model="stock.picking.type" eval="{
                    'name': 'Pre-Pick MO OPL',
                    }"/>
  </function>

</odoo>
